model Patient {
    id Int @id @default(autoincrement())
    lastName String @db.VarChar(70)
    firstName String @db.VarChar(60)
    streetNumber Int
    street String
    zipCode String
    city String
    companyId Int
    company Company @relation(fields:[companyId], references: [id])
    tourId Int?
    tour Tour? @relation(fields:[tourId], references: [id])
}